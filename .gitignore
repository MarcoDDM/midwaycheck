# Ignore bundler config
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore uploaded files in development
/storage/*
!/storage/.keep

# Ignore master key for decrypting credentials and more.
/config/master.key

# Ignore the .env file containing environment variables.
/.env

# Ignore all local precompiled assets
/public/assets
.byebug_history

# Ignore node_modules
/node_modules

# Ignore Yarn lock file
/yarn.lock

# Ignore coverage reports generated by tools like SimpleCov
/coverage

# Ignore bundler binstubs
/bin

# Ignore byebug command history file.
.byebug_history

# Ignore various editor files
*~
*.swp
*.swo
*.orig

# Ignore MacOS files
.DS_Store

# Ignore Redis dump file
/dump.rdb

# Ignore Capistrano log file
/capistrano.log

# Ignore sensitive credentials files
/config/credentials.yml.enc

/public/packs
/public/packs-test
/node_modules
/yarn-error.log
yarn-debug.log*
.yarn-integrity

/config/credentials/production.key
